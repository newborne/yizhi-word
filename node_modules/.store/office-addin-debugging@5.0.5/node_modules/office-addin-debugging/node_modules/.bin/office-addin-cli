#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/win_n/Desktop/yizhi-word/node_modules/.store/office-addin-cli@1.5.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/win_n/Desktop/yizhi-word/node_modules/.store/office-addin-cli@1.5.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../office-addin-cli@1.5.5/node_modules/office-addin-cli/cli.js" "$@"
else
  exec node  "$basedir/../../../../../office-addin-cli@1.5.5/node_modules/office-addin-cli/cli.js" "$@"
fi
