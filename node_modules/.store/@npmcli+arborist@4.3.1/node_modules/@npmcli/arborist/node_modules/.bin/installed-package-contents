#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/win_n/Desktop/yizhi-word/node_modules/.store/@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/win_n/Desktop/yizhi-word/node_modules/.store/@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli/installed-package-contents/index.js" "$@"
else
  exec node  "$basedir/../../../../../../@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli/installed-package-contents/index.js" "$@"
fi
