#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/win_n/Desktop/yizhi-word/node_modules/.store/npm-packlist@3.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/win_n/Desktop/yizhi-word/node_modules/.store/npm-packlist@3.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../npm-packlist@3.0.0/node_modules/npm-packlist/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../npm-packlist@3.0.0/node_modules/npm-packlist/bin/index.js" "$@"
fi
